{
    "name": "soyhuce/laravel-testing",
    "description": "Helpers for Laravel tests",
    "keywords": [
        "soyhuce",
        "laravel",
        "testing"
    ],
    "homepage": "https://github.com/soyhuce/laravel-testing",
    "license": "MIT",
    "authors": [
        {
            "name": "Bastien Philippe",
            "email": "bastien.philippe@soyhuce.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^9.0 || ^10.0",
        "illuminate/support": "^9.0 || ^10.0",
        "illuminate/testing": "^9.0 || ^10.0",
        "phpunit/phpunit": "^9.5",
        "spatie/invade": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.6",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0 || ^8.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Soyhuce\\Testing\\": "src"
        },
        "files": ["src/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Soyhuce\\Testing\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": "vendor/bin/php-cs-fixer fix",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "all": [
            "@cs",
            "@test",
            "@analyse"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Soyhuce\\Testing\\TestingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
